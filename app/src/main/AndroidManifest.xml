<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.witono.jogjaflight">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name=".view.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.LoginActivity"
            android:theme="@style/Theme.AppCompat.DayNight.NoActionBar" />
        <activity
            android:name=".view.AbsensiActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".view.MataKuliahActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".view.JadwalActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".view.KHSActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".view.ProfileActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".view.PembayaranActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".view.PelanggaranActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".view.PengalamanActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".view.NilaiPerformaActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".view.TabelJadwalActivity"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".view.TabelKHSActivity"
            android:theme="@style/AppTheme" />
    </application>

</manifest>